    "@types/cors": "^2.8.10",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/mongoose": "^5.10.5",
    "@types/node": "^15.3.0",
    "@types/pino": "^6.3.8",
    "@types/pino-pretty": "^4.7.0",
